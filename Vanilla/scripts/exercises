#!/usr/bin/emacs --script
(load "~/.emacs.d/lisp/straight")
(straight-use-package 'modular-config)
(straight-use-package 'use-package)
(defalias 'p! 'use-package)
(require 'modular-config)
(modular-config-load-modules '(org))
(unless (shell-command-to-string "pgrep org")
(if (org-google-tasks-check-date)
    (org-google-tasks-get-tokens)
    ))
(require 'notifications)
(with-current-buffer (find-file-noselect "~/Documents/Org/Agenda/habits.org")
  (org-ql-find "exercise")
)
